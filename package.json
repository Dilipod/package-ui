{
  "name": "@dilipod/ui",
  "version": "0.2.2",
  "description": "Dilipod Design System - Shared UI components and styles",
  "author": "Dilipod <hello@dilipod.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dilipod/package-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Dilipod/package-ui/issues"
  },
  "homepage": "https://dilipod.com",
  "keywords": [
    "design-system",
    "react",
    "components",
    "ui",
    "dilipod",
    "tailwindcss"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./src/styles/globals.css",
    "./styles.css": "./src/styles/globals.css"
  },
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "release": "npm run build && npm publish",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "8.6.14",
    "tailwindcss": "^4.1.8",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
